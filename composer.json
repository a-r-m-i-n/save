{
    "name": "t3/save",
    "description": "Save with shortcuts. Adds the possibility to use the keyboard shortcut CTRL+S to save in TYPO3 CMS backend.",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/a-r-m-i-n/save",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Armin Vieweg",
            "role": "Developer",
            "email": "armin@v.ieweg.de",
            "homepage": "https://v.ieweg.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "typo3/cms-core": "^8.7 || ^9.5 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "T3\\Save\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "save"
        }
    }
}
